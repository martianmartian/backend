 $(document).ready(function(){
    $('.modal-trigger').leanModal({
      dismissible: true,
      opacity: .5,
      in_duration: 300,
      out_duration: 200,  
      complete: function() {
          var mes={
            name:$("#name").val(),
            email:$("#email").val(),
            message:$("#message").val(),
            sub:"user question"
          };
          socket.emit("contactMe",mes)
          alert('message sent')
      } 
    });
  });
</script>




